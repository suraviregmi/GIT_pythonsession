    "print(\"start\")\n",
    "c=input(\"enter number\")\n",
    "a=int(c)\n",
    "flag=0\n",
    "i=2;\n",
    "while (i<a):\n",
    "    for r in range(2,a-1):\n",
    "        z=a%r\n",
    "        i+=1\n",
    "        if(z==0):\n",
    "            flag=1\n",
    "if (flag==0):    \n",
    "    print(\"prime number\")\n",
    "else:\n",
    "    print(\"not prime\")"
